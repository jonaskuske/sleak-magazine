{
  "name": "sleak-magazine",
  "private": true,
  "version": "1.0.9",
  "description": "Website for German design magazine \"sleak\"",
  "scripts": {
    "start": "npm run serve",
    "dev": "npm run serve",
    "serve": "parcel index.html -p 8080 --no-cache",
    "build": "parcel build index.html --no-cache",
    "deploy": "npm run build && node scripts/deploy.js",
    "lint": "eslint ./ --fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jonaskuske/sleak-magazine.git"
  },
  "keywords": [],
  "author": "Jonas Kuske",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/jonaskuske/sleak-magazine/issues"
  },
  "homepage": "https://github.com/jonaskuske/sleak-magazine#readme",
  "devDependencies": {
    "autoprefixer": "^8.6.5",
    "babel-core": "^6.26.3",
    "babel-preset-env": "^1.7.0",
    "eslint": "^4.19.1",
    "eslint-config-prettier": "^2.10.0",
    "eslint-plugin-prettier": "^2.7.0",
    "gh-pages": "^2.0.1",
    "parcel-bundler": "^1.10.3",
    "parcel-plugin-bundle-manifest": "^0.2.0",
    "parcel-plugin-clean-dist": "0.0.6",
    "parcel-plugin-static": "^1.0.0",
    "postcss-focus-visible": "^4.0.0",
    "prettier": "1.13.4",
    "simple-git": "^1.105.0",
    "write-pkg": "^3.2.0"
  },
  "dependencies": {
    "babel-polyfill": "^6.26.0",
    "focus-visible": "^4.1.5",
    "intersection-observer": "^0.5.0",
    "normalize.css": "^8.0.0",
    "smoothscroll-polyfill": "^0.4.3",
    "whatwg-fetch": "^2.0.4"
  },
  "browserslist": [
    "last 2 versions",
    "ie > 9",
    "> 1%"
  ],
  "parcel-plugin-static": {
    "source": "static",
    "destination": ""
  },
  "prettier": {
    "singleQuote": true,
    "trailingComma": "all",
    "semi": true
  },
  "eslintConfig": {
    "root": true,
    "extends": [
      "plugin:prettier/recommended"
    ],
    "parserOptions": {
      "sourceType": "module",
      "ecmaVersion": 2018
    }
  },
  "eslintIgnore": [
    ".cache/",
    "dist/"
  ],
  "deployedVersion": "1.0.9"
}
